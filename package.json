{
  "name": "Box",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "prebuild": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build": "cd android && gradlew assembleDebug",
    "test-apk": "react-native run-android --variant=release",
    "signIn": "cd android && gradlew signingReport",
    "prebundle": "echo Please Delete android/app/src/main/res/drawable and android/app/src/main/res/raw/*",
    "bundle": "cd android && gradlew bundleRelease",
    "releaseApk": "cd android && gradlew assembleRelease",
    "clean": "cd android && gradlew clean"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-firebase/app": "^21.6.1",
    "@react-native-firebase/database": "^21.6.1",
    "@react-native-firebase/firestore": "^21.6.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/material-top-tabs": "^7.1.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/stack": "^7.1.1",
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "i18next": "^24.2.0",
    "react": "18.3.1",
    "react-i18next": "^15.2.0",
    "react-native": "0.76.5",
    "react-native-actions-sheet": "^0.9.7",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-pager-view": "^6.6.1",
    "react-native-reanimated": "^3.16.6",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^5.0.0",
    "react-native-screens": "^4.4.0",
    "react-native-vision-camera": "^4.6.3",
    "react-native-vision-camera-text-recognition": "^3.1.1",
    "react-native-worklets-core": "^1.5.0",
    "react-redux": "^9.2.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.5",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
